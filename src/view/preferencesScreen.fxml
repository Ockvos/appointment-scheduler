<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="applicationPane" prefHeight="250.0" prefWidth="232.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PreferencesScreen">
   <children>
      <ComboBox fx:id="languageComboBox" layoutX="41.0" layoutY="40.0" onAction="#onLanguageComboBox" prefWidth="150.0" />
      <Button fx:id="backButton" alignment="CENTER" layoutX="14.0" layoutY="210.0" mnemonicParsing="false" onAction="#onBackButton" text="Back" />
      <TilePane fx:id="iconSelector" layoutX="68.0" layoutY="144.0" prefColumns="3" prefHeight="32.0" prefRows="1" prefWidth="96.0" />
      <Label fx:id="iconLabel" alignment="CENTER" layoutX="47.0" layoutY="125.0" prefHeight="18.0" prefWidth="138.0" text="Select Profile Picture">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="changeNotificationLabel" alignment="CENTER" layoutX="47.0" layoutY="176.0" prefHeight="18.0" prefWidth="138.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="supportLabel" layoutX="10.0" layoutY="14.0" text="English is only fully supported langauge" />
   </children>
</AnchorPane>
